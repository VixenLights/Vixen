<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net6.0</TargetFramework>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
  </PropertyGroup>

  <ItemGroup>
    <None Remove="SoundTouch.dll" />
    <None Remove="SoundTouch_x64.dll" />
    <None Remove="System.Windows.Controls.WpfPropertyGrid.dll" />
    <None Remove="System.Windows.Controls.WpfPropertyGrid.Themes.dll" />
  </ItemGroup>

  <ItemGroup>
    <Content Include="SoundTouch.dll">
      <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
    </Content>
    <Content Include="SoundTouch_x64.dll">
      <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
    </Content>
    <Content Include="System.Windows.Controls.WpfPropertyGrid.dll">
      <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
    </Content>
    <Content Include="System.Windows.Controls.WpfPropertyGrid.Themes.dll">
      <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
    </Content>
  </ItemGroup>

</Project>
